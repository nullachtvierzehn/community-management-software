{
  "name": "chatty",
  "private": true,
  "scripts": {
    "dev:frontend": "npm run --workspace frontend dev",
    "dev:backend": "npm run --workspace backend dev",
    "dev:graphql": "npm run --workspace graphql codegen:watch",
    "dev:database": "npm run --workspace database watch",
    "dev": "concurrently -k \"npm:dev:*\"",
    "our:setup": "node setup.mjs",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "patch-package"
  },
  "workspaces": {
    "packages": [
      "frontend",
      "backend",
      "database",
      "graphql"
    ]
  },
  "resolutions": {
    "grafast": "0.1.1-beta.1",
    "**/grafast": "0.1.1-beta.1"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "envfile": "^6.22.0",
    "inquirer": "^9.2.12",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/pg": "^8.10.9",
    "concurrently": "^8.2.2",
    "npm-force-resolutions": "^0.0.10",
    "patch-package": "^8.0.0"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=18.0.0",
    "vscode": "^1.22.0"
  }
}
