{
  "name": "root",
  "private": true,
  "scripts": {
    "dev:frontend": "npm run --workspace @app/frontend dev",
    "dev:backend": "npm run --workspace @app/backend dev",
    "dev:graphql": "npm run --workspace @app/graphql codegen:watch",
    "dev:database": "npm run --workspace @app/database watch",
    "dev": "concurrently -k \"npm:dev:*\"",
    "lint": "npm run --workspace @app/frontend --workspace @app/backend lint",
    "lint:fix": "npm run --workspace @app/frontend --workspace @app/backend lint:fix",
    "our:setup": "node setup.mjs",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "patch-package"
  },
  "workspaces": {
    "packages": [
      "@app/*"
    ]
  },
  "resolutions": {
    "grafast": "0.1.1-beta.1",
    "**/grafast": "0.1.1-beta.1"
  },
  "dependencies": {
    "@fastify/secure-session": "^7.1.0",
    "dotenv": "^16.3.1",
    "envfile": "^6.22.0",
    "inquirer": "^9.2.12",
    "npm-force-resolutions": "^0.0.10",
    "pg": "^8.11.3",
    "sodium-native": "^4.0.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/pg": "^8.10.9",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint_d": "^13.1.1",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "npm-force-resolutions": "^0.0.10",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.0"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=18.0.0",
    "vscode": "^1.22.0"
  }
}
